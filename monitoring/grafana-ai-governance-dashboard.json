      "list": [
        {
          "name": "DS_PROMETHEUS",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        },
        {
          "name": "risk_tier",
          "type": "query",
          "datasource": "${DS_PROMETHEUS}",
          "query": "label_values(ai_governance_operation_total, risk_tier)",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "multi": true,
          "refresh": 1
        },
        {
          "name": "criteria",
          "type": "query",
          "datasource": "${DS_PROMETHEUS}",
          "query": "label_values(ai_governance_checkpoint_total, criteria)",
