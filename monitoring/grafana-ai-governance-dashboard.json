      {
        "id": 1,
        "title": "\ud83d\udcc8 Governance Checkpoints by Status",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "sum(rate(ai_governance_checkpoint_total[5m])) by (criteria, status)",
            "legendFormat": "{{criteria}} - {{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto",
              "axisPlacement": "auto"
            },
            "unit": "ops"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": [
              "last",
              "mean"
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 2,
        "title": "\ud83c\udfaf Governance Compliance Rate",
        "type": "gauge",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "avg(ai_governance_compliance_rate) by (criteria)",
            "legendFormat": "{{criteria}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 0.7,
                  "color": "yellow"
                },
                {
                  "value": 0.9,
                  "color": "green"
                }
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "orientation": "auto",
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
