      {
        "id": 1,
        "title": "üìà Governance Checkpoints by Status",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "sum(rate(ai_governance_checkpoint_total[5m])) by (criteria, status)",
            "legendFormat": "{{criteria}} - {{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "ops"
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "right", "calcs": ["last", "mean"]}
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 2,
        "title": "üéØ Governance Compliance Rate",
        "type": "gauge",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "avg(ai_governance_compliance_rate) by (criteria)",
            "legendFormat": "{{criteria}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "orientation": "auto",
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}
      },
      {
        "id": 3,
        "title": "ü•ß AI Operations by Risk Tier",
        "type": "piechart",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "sum(ai_governance_operation_total) by (risk_tier)",
            "legendFormat": "{{risk_tier}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "hideFrom": {"tooltip": false, "viz": false, "legend": false}
            },
            "unit": "short"
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]},
          "pieType": "pie",
          "tooltip": {"mode": "single"}
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 14}
      },
      {
        "id": 4,
        "title": "‚ö° Operation Latency (P95)",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ai_governance_latency_seconds_bucket[5m])) by (operation_type, risk_tier)",
            "legendFormat": "{{operation_type}} - {{risk_tier}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 2, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "right", "calcs": ["last", "mean", "max"]}
        },
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 14}
      },
