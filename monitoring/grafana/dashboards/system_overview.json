      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "options": {
        "mode": "html",
        "content": "\n<div style=\"text-align: center; margin-bottom: 8px;\">\n    <div style=\"display: inline-flex; align-items: center; gap: 6px; flex-wrap: wrap; justify-content: center;\">\n        <span style=\"padding: 8px 16px; background: #3274D9; color: white; border-radius: 4px; font-weight: bold; margin: 0 4px;\">System Overview</span> <a href=\"/d/llm-metrics\" style=\"padding: 8px 16px; background: #1f1f1f; color: #33b5e5; border-radius: 4px; text-decoration: none; margin: 0 4px; display: inline-block; border: 1px solid #3274D9; transition: all 0.2s;\" onmouseover=\"this.style.background='#3274D9'; this.style.color='white';\" onmouseout=\"this.style.background='#1f1f1f'; this.style.color='#33b5e5';\">LLM Metrics</a> <a href=\"/d/rag-performance\" style=\"padding: 8px 16px; background: #1f1f1f; color: #33b5e5; border-radius: 4px; text-decoration: none; margin: 0 4px; display: inline-block; border: 1px solid #3274D9; transition: all 0.2s;\" onmouseover=\"this.style.background='#3274D9'; this.style.color='white';\" onmouseout=\"this.style.background='#1f1f1f'; this.style.color='#33b5e5';\">RAG Performance</a> <a href=\"/d/drift-monitoring\" style=\"padding: 8px 16px; background: #1f1f1f; color: #33b5e5; border-radius: 4px; text-decoration: none; margin: 0 4px; display: inline-block; border: 1px solid #3274D9; transition: all 0.2s;\" onmouseover=\"this.style.background='#3274D9'; this.style.color='white';\" onmouseout=\"this.style.background='#1f1f1f'; this.style.color='#33b5e5';\">Drift Monitoring</a> <a href=\"/d/alerts-overview\" style=\"padding: 8px 16px; background: #1f1f1f; color: #33b5e5; border-radius: 4px; text-decoration: none; margin: 0 4px; display: inline-block; border: 1px solid #3274D9; transition: all 0.2s;\" onmouseover=\"this.style.background='#3274D9'; this.style.color='white';\" onmouseout=\"this.style.background='#1f1f1f'; this.style.color='#33b5e5';\">Alerts Overview</a> <a href=\"/d/ai-governance-dashboard\" style=\"padding: 8px 16px; background: #1f1f1f; color: #33b5e5; border-radius: 4px; text-decoration: none; margin: 0 4px; display: inline-block; border: 1px solid #3274D9; transition: all 0.2s;\" onmouseover=\"this.style.background='#3274D9'; this.style.color='white';\" onmouseout=\"this.style.background='#1f1f1f'; this.style.color='#33b5e5';\">AI Governance</a>\n    </div>\n</div>\n"
      },
      "transparent": true
    },
    {
      "id": 1,
      "title": "Business API Requests by Endpoint",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 7
      },
      "targets": [
        {
          "expr": "sum by (endpoint) (rate(llm_request_total{endpoint!=\"embedding\",endpoint!=\"story_chat\"}[5m]))",
          "legendFormat": "LLM: {{endpoint}}"
        },
        {
          "expr": "sum(rate(embedding_duration_seconds_count[5m]))",
          "legendFormat": "Embedding"
        },
        {
          "expr": "sum by (service) (rate(inference_requests_total[5m]))",
          "legendFormat": "Inference: {{service}}"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec",
          "show": true
        },
        {
          "format": "short",
          "show": true
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "rightSide": false,
        "values": true,
        "current": true,
        "avg": true,
        "max": true
      }
    },
    {
      "id": 2,
      "title": "API Success Rate",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 7
      },
      "targets": [
        {
          "expr": "sum(rate(llm_request_total{status=\"success\"}[5m])) / sum(rate(llm_request_total[5m]))"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 0.95,
                "color": "yellow"
              },
              {
                "value": 0.99,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Total LLM Cost (Last Hour)",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 7
      },
      "targets": [
        {
          "expr": "sum(increase(llm_cost_usd_total[1h]))"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "API Latency Distribution (P50/P95/P99)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 15
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(llm_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(llm_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(llm_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration",
          "show": true
        },
        {
          "format": "short",
          "show": true
        }
      ]
    },
    {
      "id": 5,
      "title": "Error Rate by Service",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 15
      },
      "targets": [
        {
          "expr": "sum by (endpoint) (rate(llm_request_total{status=\"error\",endpoint!=\"embedding\",endpoint!=\"story_chat\"}[5m]))",
          "legendFormat": "LLM Error: {{endpoint}}"
        },
        {
          "expr": "sum by (service) (rate(inference_error_total[5m]))",
          "legendFormat": "Inference Error: {{service}}"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Errors/sec",
          "show": true
        },
        {
          "format": "short",
          "show": true
        }
      ]
    },
    {
      "id": 6,
      "title": "Total Token Usage (Last Hour)",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 23
      },
      "targets": [
        {
          "expr": "sum(increase(llm_token_usage_total[1h]))"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 7,
      "title": "Active Models",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 23
      },
      "targets": [
        {
          "expr": "count(count by (model) (llm_request_total))"
        }
      ],
      "options": {
        "graphMode": "none",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 8,
      "title": "Query Cache Hit Rate",
      "type": "gauge",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 23
      },
      "targets": [
        {
          "expr": "rate(rag_query_cache_hits_total[5m]) / (rate(rag_query_cache_hits_total[5m]) + rate(rag_query_cache_misses_total[5m]))",
          "legendFormat": "Cache Hit Rate"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 0.5,
                "color": "yellow"
              },
              {
                "value": 0.8,
                "color": "green"
              }
            ]
          },
          "decimals": 2
        }
      }
    },
    {
      "id": 9,
      "title": "RAG Success Rate",
      "type": "gauge",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 23
      },
      "targets": [
        {
          "expr": "sum(rate(rag_request_total{status=\"success\"}[5m])) / sum(rate(rag_request_total[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 0.95,
                "color": "yellow"
              },
              {
                "value": 0.99,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Request Volume by Endpoint",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 29
      },
      "targets": [
        {
          "expr": "sum(rate(llm_request_total[5m])) by (endpoint)",
          "legendFormat": "{{endpoint}}"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec",
          "show": true
        }
      ]
    },
    {
      "id": 11,
      "title": "Cost by Model (Last Hour)",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 29
      },
      "targets": [
        {
          "expr": "sum(increase(llm_cost_usd_total[1h]) > 0) by (model)"
        }
      ]
    },
    {
      "id": 12,
      "title": "Inference Service Health",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 37
      },
      "targets": [
        {
          "expr": "up{job=\"ai-louie-backend\"}",
          "legendFormat": "{{service}}"
        }
      ],
      "options": {
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "Healthy",
                  "color": "green"
                }
              }
            },
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "Down",
                  "color": "red"
                }
              }
            },
            {
              "type": "value",
              "options": {
                "2": {
                  "text": "Recovering",
                  "color": "yellow"
                }
              }
            }
          ]
        }
      }
    },
    {
      "id": 13,
      "title": "Top 5 Slowest Endpoints (P95)",
      "type": "table",
      "gridPos": {
        "h": 6,
        "w": 16,
        "x": 8,
        "y": 37
      },
      "targets": [
        {
          "expr": "topk(5, histogram_quantile(0.95, sum(rate(llm_request_duration_seconds_bucket[5m])) by (endpoint, le)))",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true
            },
            "renameByName": {
              "endpoint": "Endpoint",
              "Value": "P95 Latency (s)"
            }
          }
        }
      ]
    }
  ],
  "uid": "system-overview"
}