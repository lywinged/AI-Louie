      "title": "Chat Interactions - Success Rate (Last Hour)",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 7
      },
      "targets": [
        {
          "expr": "sum(rate(llm_request_total{endpoint=\"rag\",status=\"success\"}[1h])) / sum(rate(llm_request_total{endpoint=\"rag\"}[1h]))"
