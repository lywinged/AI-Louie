        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 0.95,
                "color": "yellow"
              },
              {
                "value": 0.99,
